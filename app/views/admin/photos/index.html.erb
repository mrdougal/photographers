<% title "Photos #{pagination_title @photos}" %>


<%= will_paginate @photos %>

<% @photos.each do |photo| %>
    <% link_to admin_photo_path(photo.permalink) do %>
		<%= image_tag photo.url(:thumb), { :class => "uploaded" } %>
	<% end %>
<% end %>


<%= will_paginate @photos %>


<%= render :partial => 'crud' %>
