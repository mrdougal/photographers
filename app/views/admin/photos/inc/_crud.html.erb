<% content_for :options do %>

<div class="crud">
                      
	<% if @photo && !@photo.new_record? %>
	                  
	<%= link_to 'Edit', edit_admin_photo_path(@photo), { 
		:class => 'edit', 
		:accesskey => 'e' } unless controller.action_name == 'edit'  %>
	
	<%= link_to 'Delete', admin_photo_path(@photo), { 
		:confirm => 'You sure you want to delete this photo?', 
		:method => :delete ,
		:class => 'delete', 
		:accesskey => 'd' } %>
	<% end %>         

</div>
 

<% end %>

