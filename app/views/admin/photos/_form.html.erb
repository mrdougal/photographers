
<!-- We need a multipart form as we're uploading images -->

<% semantic_form_for [:admin, @photo] do |f| %>  

	<% f.inputs do %>

		<%= f.input :category_list, :as => :string %>
		<%= f.input :set_list, :as => :string %>
		<%= f.input :tag_list, :as => :string %>
		
		<%= f.buttons %>
	
	<% end %>

	
<% end %>
